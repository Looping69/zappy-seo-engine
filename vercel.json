{
    "version": 2,
    "cleanUrls": true,
    "framework": "vite",
    "rewrites": [
        {
            "source": "/keywords(.*)",
            "destination": "https://zappy-seo-engine-r64i.encoreapi.com/keywords$1"
        },
        {
            "source": "/content(.*)",
            "destination": "https://zappy-seo-engine-r64i.encoreapi.com/content$1"
        },
        {
            "source": "/orchestrator(.*)",
            "destination": "https://zappy-seo-engine-r64i.encoreapi.com/orchestrator$1"
        },
        {
            "source": "/(.*)",
            "destination": "web/dist/$1"
        }
    ]
}